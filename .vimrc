execute pathogen#infect()
syntax enable
let g:solarized_termcolors=256
set background=dark
colorscheme solarized
filetype plugin indent on
set nu

inoremap jk <ESC>

set expandtab
set shiftwidth=4
set softtabstop=4
set autoindent
set smartindent

autocmd FileType html set omnifunc=htmlcomplete#CompleteTags 
